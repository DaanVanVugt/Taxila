<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Link</th>
      <th>Provider</th>
      <th>Field</th>
      <th>Description</th>
      <th>Category</th>
      <th>Start</th>
      <th>End</th>
      <th>Sponsor</th>
      <th>Venue</th>
      <th>City</th>
      <th>County</th>
      <th>Country</th>
      <th>Postcode</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.title %></td>
        <td><%= event.subtitle %></td>
        <td><%= event.link %></td>
        <td><%= event.provider %></td>
        <td><%= event.field %></td>
        <td><%= event.description %></td>
        <td><%= event.category %></td>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        <td><%= event.sponsor %></td>
        <td><%= event.venue %></td>
        <td><%= event.city %></td>
        <td><%= event.county %></td>
        <td><%= event.country %></td>
        <td><%= event.postcode %></td>
        <td><%= event.latitude %></td>
        <td><%= event.longitude %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
